#print screen
print("Hey Buddy!")

#Arithmatic operators/Assignment
a=11*6+7-10/10**6//66
b=6^7*10
print(a-b)

#comparison operator
print('a>b', a>b)

#Logical Operator
x=True
y=False
print('x and y', x and y)

#indexing and slicing
String="Helo Machine"
print(String[0])
print(String[-1])
print(String[:8])
print(String[8:])
print(String[0:-1])

#concatenation
a="Hello"
b="World"
print(a+b)
print(a[:3]=b[2:0])

#split and join
s="try to split me"
print(s.split(" "))

s= " ".join(s)
print(s)

#uppercase and lowercase 
print(s.upper())
print(s.lower())


#TUPLES operations
tup= '1','2','3','4','5'
tup2= 'A','B','C','D','E'
print(tup[2])
tup3= (tup,tup2)
print(tup3)

